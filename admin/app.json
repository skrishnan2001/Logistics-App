{
  "expo": {
    "name": "LogisticsAdmin",
    "slug": "LogisticsAdmin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/delivery_truck.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
